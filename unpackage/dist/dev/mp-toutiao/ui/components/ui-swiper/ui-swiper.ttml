<view><view class="{{((('ui-swiper')+' '+mode)+' '+bg)}}"><swiper style="{{'height:'+(height)+';'}}" circular="{{circular}}" autoplay="{{true}}" interval="{{interval}}" duration="500" data-event-opts="{{[['change',[['swiperChange',['$event']]]]]}}" bindchange="__e"><block tt:for="{{info}}" tt:for-item="item" tt:for-index="index" tt:key="index"><swiper-item class="{{((cur==index)?'cur':'')}}"><view class="ui-swiper-main radius"><image style="{{'height:'+(imgHeight)+';'}}" src="{{item.img}}" mode="aspectFill"></image></view></swiper-item></block></swiper><block tt:if="{{dotStyle!='tag'}}"><view class="{{(('ui-swiper-dot')+' '+dotStyle)}}"><block tt:for="{{info}}" tt:for-item="item" tt:for-index="index" tt:key="index"><view class="{{((cur==index?'cur':'')+' '+dotCur)}}"></view></block></view></block><block tt:if="{{dotStyle=='tag'}}"><view class="{{(('ui-swiper-dot')+' '+dotStyle)}}"><view class="{{(('ui-tag radius')+' '+dotCur)}}"><view>{{cur+1+" / "+info.length}}</view></view></view></block></view></view>
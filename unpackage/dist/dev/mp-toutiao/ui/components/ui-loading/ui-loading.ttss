.ui-loading-box .ui-loading {
  width: 1em;
  height: 1em;
  margin: 0 0.5em;
  -webkit-animation: loading_spin 0.7s linear infinite;
          animation: loading_spin 0.7s linear infinite;
}
.ui-loading-box .ui-loading .ui-loading-cut {
  position: relative;
  width: 0.5em;
  height: 1em;
  overflow: hidden;
  position: absolute;
  top: 0;
  left: 0;
}
.ui-loading-box .ui-loading .ui-loading-cut .ui-loading-loop {
  box-sizing: border-box;
  width: 1em;
  height: 1em;
  border: 2px solid currentColor;
  border-radius: 50%;
  border-left-color: transparent;
  border-bottom-color: transparent;
  position: absolute;
  top: 0;
  left: 0;
  background: none;
  margin: 0;
}
.ui-loading-box .ui-loading .ui-loading-cut .ui-loading-loop.default {
  -webkit-animation: loop_spin 2s cubic-bezier(0.4, 0, 0.22, 1) infinite;
          animation: loop_spin 2s cubic-bezier(0.4, 0, 0.22, 1) infinite;
}
.ui-loading-box .ui-loading .ui-loading-cut .ui-loading-loop.color {
  -webkit-animation: loop_spin_color 2s cubic-bezier(0.4, 0, 0.22, 1) infinite;
          animation: loop_spin_color 2s cubic-bezier(0.4, 0, 0.22, 1) infinite;
}
.ui-loading-box.loop {
  position: relative;
}
.ui-loading-box.loop .ui-loading {
  position: relative;
  z-index: 1;
  width: 6em;
  height: 6em;
  margin: 0;
}
.ui-loading-box.loop .ui-loading .ui-loading-cut {
  width: 3em;
  height: 6em;
}
.ui-loading-box.loop .ui-loading .ui-loading-cut .ui-loading-loop {
  width: 6em;
  height: 6em;
}
.ui-loading-box.loop .ui-loading-content {
  position: absolute;
  border-radius: 50%;
  width: 6em;
  height: 6em;
  left: 0;
  top: 0;
  border: 2px solid var(--ui-BG-3);
}
.ui-loading-box.loop .ui-loading-content image {
  width: 80% !important;
  height: 80% !important;
  margin: 10%;
}
@-webkit-keyframes loading_spin {
0% {
    -webkit-transform: rotate(0deg);
            transform: rotate(0deg);
}
100% {
    -webkit-transform: rotate(360deg);
            transform: rotate(360deg);
}
}
@keyframes loading_spin {
0% {
    -webkit-transform: rotate(0deg);
            transform: rotate(0deg);
}
100% {
    -webkit-transform: rotate(360deg);
            transform: rotate(360deg);
}
}
@-webkit-keyframes loop_spin {
0% {
    -webkit-transform: rotate(0);
            transform: rotate(0);
}
50% {
    -webkit-transform: rotate(-140deg);
            transform: rotate(-140deg);
}
100% {
    -webkit-transform: rotate(0);
            transform: rotate(0);
}
}
@keyframes loop_spin {
0% {
    -webkit-transform: rotate(0);
            transform: rotate(0);
}
50% {
    -webkit-transform: rotate(-140deg);
            transform: rotate(-140deg);
}
100% {
    -webkit-transform: rotate(0);
            transform: rotate(0);
}
}
@-webkit-keyframes loop_spin_color {
0% {
    -webkit-transform: rotate(0);
            transform: rotate(0);
    color: #3eb93b;
}
50% {
    -webkit-transform: rotate(-140deg);
            transform: rotate(-140deg);
    color: #0081ff;
}
100% {
    -webkit-transform: rotate(0);
            transform: rotate(0);
    color: #e54d42;
}
}
@keyframes loop_spin_color {
0% {
    -webkit-transform: rotate(0);
            transform: rotate(0);
    color: #3eb93b;
}
50% {
    -webkit-transform: rotate(-140deg);
            transform: rotate(-140deg);
    color: #0081ff;
}
100% {
    -webkit-transform: rotate(0);
            transform: rotate(0);
    color: #e54d42;
}
}

